{
    "project": {
        "name": "excelGO",
        "desc": "一款简单的excel数据处理工具",
        "width": 650,
        "height": 750,
        "theme": "litera",
        "resizable": false,
        "icon": "resources\\icon\\favicon.ico",
        "home_logo": "resources\\icon\\home.png",
        "exract_icon": "resources\\icon\\save.png",
        "notebook_theme": "primary",
        "site": "https://github.com/SSymbo1/excelGO/releases",
        "default_extract": "INSERT",
        "exract_options": [
            "INSERT",
            "UPDATE"
        ],
        "default_extract_config": "数字自增",
        "extract_config": [
            "数字自增",
            "字母自增",
            "表格取值",
            "自定义"
        ],
        "extract_config_table": [
            "列名",
            "取值方式",
            "取值"
        ]
    },
    "extract": {
        "default_name": "dump_excel_{timestamp}.sql",
        "export_path": "export",
        "sql_case": "lower",
        "auto_number": 0,
        "auto_character": "a",
        "header_column": 1,
        "data_column": 2
    }
}